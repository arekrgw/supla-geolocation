{
  "expo": {
    "name": "Supla Geolocation",
    "slug": "supla-geolocation",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "0.5",
    "orientation": "default",
    "icon": "./assets/appicon.png",
    "splash": {
      "image": "./assets/appsplash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "notification": {
      "icon": "./assets/appiconnoti.png",
      "androidMode": "collapse",
      "androidCollapsedTitle": "Powiadomienia ze stref"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "splash": {
        "image": "./assets/appsplash.png",
        "resizeMode": "native",
        "backgroundColor": "#000"
      },
      "package": "pl.suplageolocation",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAfs1eavA4GsRU_lOA47ViU6ozXSVHZTN8"
        }
      }
    }
  }
}
