{
  "expo": {
    "name": "Supla Geolocation",
    "slug": "supla-geolocation",
    "privacy": "public",
    "sdkVersion": "34.0.0",
    "platforms": ["ios", "android", "web"],
    "version": "1.0.0",
    "orientation": "default",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["location", "fetch"]
      }
    },
    "android": {
      "package": "pl.suplageolocation",
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyAfs1eavA4GsRU_lOA47ViU6ozXSVHZTN8"
        }
      }
    }
  }
}
